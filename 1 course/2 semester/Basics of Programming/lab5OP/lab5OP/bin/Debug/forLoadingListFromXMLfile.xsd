<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="http://www.ipm.ifmo.ru" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="ArrayOfEmployee">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" name="Employee">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="SpecialNumber" type="xs:positiveInteger" />
              <xs:element name="Name" type="xs:string" />
              <xs:element name="LastName" type="xs:string" />
              <xs:element name="Patronymic" type="xs:string" />
              <xs:element name="BirthDate" type="xs:string" />
              <xs:element name="Position" type="xs:string" />
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>